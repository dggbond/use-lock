{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "es5",
      "es2015",
      "es2016",
      "dom",
      "esnext"
